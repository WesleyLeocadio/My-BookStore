<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mybookstore">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".swipe_drag_drop.SwipeDragDropActivity"></activity>
        <activity android:name=".grid_view.GridViewActivity" />
        <activity android:name=".view_pager.ViewPagerActivity" />
        <activity android:name=".recycler_view.RecyclerViewActivity" />
        <activity android:name=".list_view.ListViewActivity" />
        <activity android:name=".auto_complete_textview.AutoCompleteTextViewActivity" />
        <activity android:name=".search_books.SearchBooksActivity" />
        <activity android:name=".register.RegisterActivity" />
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>